  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ilkhomkhafizov/mern-project/issues"
  },
  "homepage": "https://github.com/ilkhomkhafizov/mern-project#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.5",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.8.9",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
